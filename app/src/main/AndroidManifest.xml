<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.geekym.eatfit">

    <application
        android:allowBackup="true"
        android:icon="@drawable/finalicon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/finalicon"
        android:supportsRtl="true"
        android:theme="@style/Theme.EatFit">
        <activity android:name="com.geekym.eatfit.result" />
        <activity android:name="com.geekym.eatfit.food_planner" />
        <activity android:name="com.geekym.eatfit.FoodOrder" />
        <activity android:name="com.geekym.eatfit.HomeScreen" />
        <activity android:name="com.geekym.eatfit.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.geekym.eatfit.MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>